{
    "name": "node-template",
    "version": "1.0.0",
    "description": "A small project with big dreams",
    "main": "server.js",
    "scripts": {
        "start:prod": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/index.js",
        "start:dev": "ts-node -r tsconfig-paths/register src/index.ts",
        "dev": "nodemon src/index.ts",
        "build": "tsc -p .",
        "test": "jest",
        "lint": "npm run lint:js",
        "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
        "lint:js": "npm run lint:eslint src/",
        "lint:fix": "npm run lint:js -- --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/iitsuraj/node-template.git"
    },
    "keywords": [
        "cms",
        "infogile",
        "node-template"
    ],
    "author": "iitsuraj",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/iitsuraj/node-template/issues"
    },
    "homepage": "https://github.com/iitsuraj/node-template#readme",
    "dependencies": {
        "app-module-path": "^2.2.0",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "lodash": "^4.17.20",
        "mongoose": "^5.10.6"
    },
    "devDependencies": {
        "@types/express": "^4.17.8",
        "@types/jest": "^26.0.14",
        "@types/lodash": "^4.14.161",
        "@types/mongoose": "^5.7.36",
        "@types/node": "^14.11.1",
        "@typescript-eslint/eslint-plugin": "^4.2.0",
        "@typescript-eslint/parser": "^4.2.0",
        "eslint": "^7.9.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "jest": "^26.4.2",
        "morgan": "^1.10.0",
        "nodemon": "^2.0.4",
        "prettier": "^2.1.2",
        "ts-jest": "^26.4.0",
        "ts-node": "^9.0.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.0.3",
        "@types/cors": "^2.8.7",
        "@types/morgan": "^1.9.1"
    }
}
